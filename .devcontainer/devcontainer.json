			"version": "latest"
		},
		"ghcr.io/devcontainers-contrib/features/aztfexport:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers-contrib/features/ballerina-sdkman:2": {
			"version": "latest",
			"jdkVersion": "8",
			"jdkDistro": "ms"
		},
