			"version": "latest",
			"jdkVersion": "17",
			"jdkDistro": "ms"
		},
		"ghcr.io/devcontainers-contrib/features/apko:1": {
			"version": "latest"
		},
		"ghcr.io/devcontainers-contrib/features/apt-get-packages:1": {
			"clean_ppas": true,
			"preserve_apt_list": true,
			"force_ppas_on_non_ubuntu": true,
			"packages": "curl,ca-certificates",
			"ppas": "ppa:deadsnakes/ppa"
		},
		"ghcr.io/devcontainers-contrib/features/apt-packages:1": {
			"clean_ppas": true,
			"preserve_apt_list": true,
			"packages": "htop",
			"ppas": "ppa:deadsnakes/ppa"
		},
		"ghcr.io/devcontainers-contrib/features/argo-cd:1": {
			"version": "2.5.0"
		},
		"ghcr.io/devcontainers-contrib/features/argo-workflows:1": {
			"version": "latest"
		},
